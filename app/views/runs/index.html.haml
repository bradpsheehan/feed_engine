#inner-container.span8
  #inner-header.span8
    %h1 Your Groups
  -@runs.each do |run|
    #inner-content.span8
      #run-details
        .run-header.pull-left
          %h1
            =run.name
        .pull-right
          %h3 Start Time
          %p
            =run.run_start_time
          %h3 Date
          %p
            =run.run_date
